import java.util.*;
import java.lang.*;
import java.io.*;

 class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s= new Scanner(System.in);
		int T= s.nextInt();
		
		for(int i=1;i<=T;i++){
		    int X=s.nextInt();
		    if(X<=10){
		    System.out.println("yes");
		    }
		    else{System.out.println("NO");
		 }
	 }
 }
}
